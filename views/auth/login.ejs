<body>
    <div class="login">
        <div class="login-body">
            <a class="text-center" href="">
                <h3>ggg </h3>
            </a>
            <h3 class="login-heading"><%= 'ورود' %></h3>
            <div class="login-form">
                <form data-toggle="validator" method="POST" action="">
                    <div class="form-group">
                        <label for="userName" class="control-label"> <%= 'نام کاربری' %>  </label>
                        <input id="userName" class="form-control" type="text" name="userName" spellcheck="false"
                            autocomplete="off" data-msg-required="Please enter your username." required>
                    </div>
                    <div class="form-group">
                        <label for="password" class="control-label"><%= 'گذر واژه' %></label>
                        <input id="password" class="form-control" type="password" name="password" minlength="6"
                            data-msg-minlength="<%= 'password must be 6 characters or more' %>"
                            data-msg-required="<%= 'please enter your password' %>" required>
                    </div>

                    <div class="form-group">
                        <button class="btn btn-primary btn-block" type="submit"><%= 'ورود' %></button>
                    </div>
                    <div class="form-group">
